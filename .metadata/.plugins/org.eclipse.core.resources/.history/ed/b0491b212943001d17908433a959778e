package com.javalec.dto;

public class MypageDto {
	// fields
	String cpassword;
	String cname;
	String cinitdate;
	String cupdatedate;
	String cdeletedate;
	String custid;

	public MypageDto() {

	}

//사용하고 자하는 사이즈들을 지정해줌 

	public MypageDto(String cname, String cinitdate, String custid) {
		super();
		this.cname = cname;
		this.cinitdate = cinitdate;
		this.custid = custid;
	}

	public MypageDto(String cdeletedate) {
		super();
		this.cdeletedate = cdeletedate;
	}

	public String getCpassword() {
		return cpassword;
	}

	public void setCpassword(String cpassword) {
		this.cpassword = cpassword;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public String getCinitdate() {
		return cinitdate;
	}

	public void setCinitdate(String cinitdate) {
		this.cinitdate = cinitdate;
	}

	public String getCupdatedate() {
		return cupdatedate;
	}

	public void setCupdatedate(String cupdatedate) {
		this.cupdatedate = cupdatedate;
	}

	public String getCdeletedate() {
		return cdeletedate;
	}

	public void setCdeletedate(String cdeletedate) {
		this.cdeletedate = cdeletedate;
	}

	public String getCustid() {
		return custid;
	}

	public void setCustid(String custid) {
		this.custid = custid;
	}

}